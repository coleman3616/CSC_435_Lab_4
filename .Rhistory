library(readr)
Hospital_Database <- read_csv("Hospital Database.csv")
View(Hospital_Database)
library(tidyverse)
pysch <- filter(data = Hospital_Database, Service ="0")
pysch <- filter(data = Hospital_Database, Service =="0")
pysch <- filter(data = Hospital_Database, Hospital_Database$Service =="0")
pysch <- filter(Hospital_Database$Service =="0")
pysch <- Hospital_Database %>% filter(Service =="0")
general <- Hospital_Database %>% filter(Service =="1")
model <- lm(Service ~ Beds + Admissions + `Outpatient Visits` + Births + Personnel + `Total Expenditure`,data = pysch)
summary(model)
model <- lm(`Total Expenditure` ~ Personnel ,data = Hospital_Database)
summary(model)
model2 <- lm(`Total Expenditure` ~ Personnel + Beds + Admissions + `Outpatient Visits` + Births ,data = Hospital_Database)
summary(model2)
model2 <- lm(`Total Expenditure` ~ Personnel + Beds + Admissions +Births ,data = Hospital_Database)
summary(model2)
model2 <- lm(`Total Expenditure` ~ Personnel +Admissions +Births ,data = Hospital_Database)
summary(model2)
##Checking for collinearity
model2 <- lm(`Total Expenditure` ~ Personnel + Admissions,data = Hospital_Database)
summary(model2)
model2 <- lm(`Total Expenditure` ~ Personnel + Births ,data = Hospital_Database)
summary(model2)
model2 <- lm(`Total Expenditure` ~ Admissions +Births ,data = Hospital_Database)
summary(model2)
install.packages("caret")
library(caret)
vif()
install.packages("car")
library(car)
vif(model2)
